<?xml version='1.0' encoding='UTF-8'?>
<Settings>
	<Module name="FIFE">
		<Setting name="FullScreen" type="bool"> False </Setting>
		<Setting name="ScreenResolution" type="str">1024x768</Setting>
		
		
		<Setting name="PlaySounds" type="bool"> True </Setting>
		<Setting name="InitialVolume" type="float"> 5.0 </Setting>
		<Setting name="PlayMusic" type="bool"> False </Setting>
		
		
		<Setting name="RenderBackend" type="str"> OpenGL </Setting>
		<Setting name="BitsPerPixel" type="int"> 0 </Setting>
		<Setting name="SDLRemoveFakeAlpha" type="bool"> True </Setting>
		<Setting name="GLCompressImages" type="bool"> False </Setting>
		<Setting name="GLUseFramebuffer" type="bool"> True </Setting>
		<Setting name="GLUseNPOT" type="bool"> False </Setting>
		<Setting name="ColorKeyEnabled" type="bool"> False </Setting>
		
		<Setting name="WindowTitle" type="str"> Fallen Heaven </Setting>
		<Setting name="WindowIcon" type="str"> gui/icons/boy.png </Setting>
		
		<Setting name="Font" type="str"> fonts/FreeSans.ttf </Setting>
		<Setting name="FontGlyphs" strip="0" type="str"> abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789.,!?-+/():;
		%&amp;`'*#=[]\"</Setting>
		<Setting name="DefaultFontSize" type="int"> 16 </Setting>
		
		<Setting name="LogModules" type="list"> controller ; video ; audio ; script ; gui ; console ; loaders ; savers ; model ; instance ; location ; cellgrid ; pathfinder ; util ; camera ; xml ; exception ; squaregrid </Setting>
		<Setting name="LogToPrompt" type="bool"> True </Setting>
		<Setting name="LogToFile" type="bool"> False </Setting>
		<Setting name="LogLevelFilter" type="int"> 0 </Setting>
		<Setting name="PychanDebug" type="bool"> True </Setting>
		
 		<Setting name="UsePsyco" type="bool"> False </Setting>
 		
		<Setting name="ProfilingOn" type="bool"> False </Setting>
		<Setting name="Lighting" type="int"> 0 </Setting>
		
	</Module>

	<Module name="rio">
		<Setting name="MapFile" type="str"> maps/shrine2.xml </Setting>

		<Setting name="CoordinateLayerName" type="str"> TechdemoMapTileLayer </Setting>
		<Setting name="QuadTreeLayerName" type="str"> </Setting>

		<Setting name="TestCameraPlacement" type="int"> 0 </Setting>
		<Setting name="TestRotationLayerName" type="str"> TechdemoMapTileLayer  </Setting>
		<Setting name="TestAgentSpeed" type="float"> 0.5 </Setting>

        <!--Added by Jon-->
        <!--<Setting name="CursorDefault" type="str"> misc/cursorDefault.png </Setting>-->
        <Setting name="CursorAttack" type="str"> misc/cursorAttack.png </Setting>
	</Module>
</Settings>
